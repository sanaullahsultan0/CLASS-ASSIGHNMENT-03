#include <iostream>
using namespace std;
int main() {
    char alp;
    cout<<"enter a singal alphabet:   ";
    cin>>alp;
    
    if((alp>='A' && alp<='Z')||(alp>='a' && alp<='z')){
        if(alp>='A' && alp<='Z'){
            if(alp=='A'||alp=='E'||alp=='I'||alp=='O'||alp=='U'){
                cout<<"upper case VOWEL:  ";
            }else{
                cout<<"upper case CONSTANT:  ";
            }
        }
        if(alp>='a'&&alp<='z'){
            if(alp=='a'||alp=='e'||alp=='i'||alp=='o'||alp=='u'){
                cout<<"lower  case vowel:  ";
            }else{
                cout<<"lower case constant:  ";
            }
        }
        
    }else{
        cout<<"not a alphebat";
    }
    
    
    
    return 0;
}
